<h2 class = "marker">ユーザー情報編集</h2>
<%= form_with model:@user, local:true do |f| %>
<h4>ユーザー画像</h4>
<%= f.attachment_field :image %>
<h4 class="user-name">ユーザー名：</h4>
<%= f.text_field :name %>
<h4 class="email">メールアドレス：</h4>
<%= f.text_field :email %>
<div>
<%= f.submit '変更を保存' %>
</div>
<% end %>



